<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00DDE3E7"
    tools:context=".MainActivity">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="16dp"
        app:cardCornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/signOutImageView"
        android:layout_width="49dp"
        android:layout_height="38dp"
        android:layout_gravity="end"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_buttom"
        android:contentDescription="@string/all"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.921"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_sign_out"
        tools:ignore="VectorDrawableCompat" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.12">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MiMedicoButton01"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/bottom_mimedico"
            android:textColor="@color/white" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisRecetasButton02"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/bottom_misrecetas"
            android:textColor="@color/white" />


    </LinearLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="213dp"
        android:layout_height="155dp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112"
        app:srcCompat="@drawable/logo2" />


    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        app:layout_constraintVertical_bias="0.078">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisMedicamentosButton03"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="@string/bottom_mismedicamentos"
            android:textColor="@color/white" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/RegistrarCitaButton04"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="Registrar cita"
            android:textColor="@color/white" />


    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="346dp"
        android:layout_height="125dp"
        android:layout_marginTop="330dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.369"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.178">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/MisCitasButton05"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="left"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="mis citas"
            android:textColor="@color/white" />


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/UbicacionButton06"
            android:layout_width="150dp"
            android:layout_height="120dp"
            android:layout_gravity="right"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp"
            android:background="@drawable/ic_buttom"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:text="Ubicacion"
            android:textColor="@color/white" />

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>